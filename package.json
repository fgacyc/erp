		"firebase": "^9.19.1",
		"formik": "^2.4.3",
		"idb-keyval": "^6.2.1",
		"octokit": "^3.1.0",
		"openapi-fetch": "^0.7.4",
		"pubsub-js": "^1.9.4",
		"qrcode.react": "^3.1.0",
		"react": "^18.2.0",
		"react-chartjs-2": "^5.2.0",
